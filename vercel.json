{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/public/(.*)", 
        "dest": "/public/$1"
      },
      {
        "src": "/(.*).html", 
        "dest": "/public/$1.html"
      },
      {
        "src": "/(.*)", 
        "dest": "/index.js"
      }
    ]
  }
  